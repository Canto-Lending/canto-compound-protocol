    "hardhat": "^2.9.7",
    "jest-diff": "^26.4.2",
    "jest-junit": "^11.1.0",
    "solium": "^1.2.5",
    "solparse": "^2.2.8",
    "ts-node": "^8.10.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "esbuild": "^0.14.42",
    "eth-saddle": "^0.1.25",
    "hardhat-deploy": "^0.11.10",
